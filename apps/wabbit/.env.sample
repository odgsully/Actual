# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Google Maps (Required for map features)
# 1. Go to https://console.cloud.google.com
# 2. Create a new project or select existing
# 3. Enable these APIs: Maps JavaScript API, Places API, Drawing Library
# 4. Create an API key under Credentials
# 5. Add localhost:3000 to allowed HTTP referrers
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=Wabbit

# Development
NODE_ENV=development

# Production-only Variables (Optional)
# CLOUDFLARE_API_TOKEN=your_cloudflare_api_token
# CLOUDFLARE_ZONE_ID=your_cloudflare_zone_id
# RESEND_API_KEY=your_resend_api_key
# SENDGRID_API_KEY=your_sendgrid_api_key
# SENTRY_DSN=your_sentry_dsn
# NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Datadog APM Configuration
# Sign up at https://datadoghq.com (free tier: 1M traces/month)
# Get API key from: Organization Settings > API Keys
DD_API_KEY=your_datadog_api_key_here
DD_SITE=datadoghq.com
DD_SERVICE=wabbit
DD_ENV=development
DD_VERSION=1.0.0
# Optional: Disable tracing in development
# DD_TRACE_ENABLED=false
# DD_PROFILING_ENABLED=true
# DD_APPSEC_ENABLED=true