{
  "buildCommand": "cd apps/gsrealty-client && npm run build",
  "outputDirectory": "apps/gsrealty-client/.next",
  "installCommand": "cd apps/gsrealty-client && npm install",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/cron/hourly-scrape",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/daily-cleanup",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron/check-health",
      "schedule": "*/15 * * * *"
    }
  ],
  "functions": {
    "app/api/cron/hourly-scrape/route.ts": {
      "maxDuration": 300
    },
    "app/api/cron/daily-cleanup/route.ts": {
      "maxDuration": 60
    },
    "app/api/cron/check-health/route.ts": {
      "maxDuration": 10
    },
    "app/api/scrape/test/route.ts": {
      "maxDuration": 60
    },
    "app/api/scrape/on-demand/route.ts": {
      "maxDuration": 300
    },
    "app/api/admin/reportit/upload/route.ts": {
      "maxDuration": 60
    },
    "app/api/admin/mcao/bulk/route.ts": {
      "maxDuration": 120
    }
  }
}